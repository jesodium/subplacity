{
  "name": "subplacity",
  "version": "1.0.0",
  "description": "roblox subplace finder",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "form-data": "^4.0.0"
  },
  "build": {
    "appId": "com.misunderstood.subplaces",
    "productName": "subplacity",
    "win": {
      "target": "portable",
      "icon": "icon.ico",
      "signAndEditExecutable": false,
      "sign": null
    },
        "mac": {
      "target": "dmg",
      "icon": "icon.png",
      "category": "public.app-category.utilities"
    },
    "files": [
      "**/*",
      "node_modules/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
