{
  "name": "subplacity",
  "productName": "Subplacity",
  "version": "1.0.0",
  "description": "roblox subplace finder",
  "main": "main.js",
  "author": "misunderstood",
  "license": "EPL-2.0",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder -mw"
  },
  "build": {
    "appId": "com.misunderstood.subplacity",
    "productName": "Subplacity",
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "Subplacity"
    }
  },
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}